@model List<Model.EF.Menu>
    <div class="headertop_desc">
        <div class="call">
            <p><span>Need help?</span> call us <span class="number">1-22-3456789</span></span></p>
        </div>
        <div class="account_desc">
            <ul>
                @{
                    var session = (UserLogin)Session[MyShop.Common.Constants.USER_SESSION];
                }
                @if (Session[MyShop.Common.Constants.USER_SESSION] != null)
                {
                    <li><a href="#">@session.UserName</a></li>
                    <li><a href="/dang-xuat">Đăng Xuất</a></li>
                }
                else
                {
                    foreach (var item in Model)
                    {
                        <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>

                    }
                }

            </ul>
        </div>
        <div class="clear"></div>
    </div>
